{
  "name": "rabbit-mq",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/rabbit-mq/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/rabbit-mq",
        "tsConfig": "libs/rabbit-mq/tsconfig.lib.json",
        "packageJson": "libs/rabbit-mq/package.json",
        "main": "libs/rabbit-mq/src/index.ts",
        "assets": ["libs/rabbit-mq/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs rabbit-mq {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/rabbit-mq/jest.config.ts"
      }
    }
  },
  "tags": []
}
